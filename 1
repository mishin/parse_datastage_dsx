         Name "USERSQL"
         Value =+=+=+=
SELECT A.CTNUMDOG, A.REPDATE, A.REF, MNTEXPOCOUR, decimal(value(sum(B.AMOUNT),0),15) as SUMMA, A.DATEFIN, B.CCY, A.CTBORINN FROM
 #PS_BCE_CONNECTIONS.TRG_SCHEME#.SG_BCE_OPEELEMCRED_B_ES_ACCRED AS A 
LEFT OUTER JOIN (SELECT PAY_DATE, CCY, REPDATE, CTNUMDOG,REF, AMOUNT
 FROM #PS_BCE_CONNECTIONS.TRG_SCHEME#.SG_BCE_ACCRED_PLAN WHERE PAY_DATE>'#DATE#') 
 AS B 
ON B.REF=A.REF and A.REPDATE=B.REPDATE and B.PAY_DATE<=A.DATEFIN
group by A.CTNUMDOG, A.REPDATE, A.REF, MNTEXPOCOUR, A.DATEFIN, B.CCY, A.CTBORINN;
=+=+=+=
