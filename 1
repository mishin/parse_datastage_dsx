record
(
  DAI_NUMBER:string[max=20];
  SPM_NUMBER:nullable string[max=50];
  LOCAL_CLIENT_ID:nullable string[max=50];
  FULL_NAME:nullable string[max=250];
  SHORT_NAME:nullable string[max=50];
  NAME:nullable string[max=40];
  CLIENT_COUNTRY_CODE:nullable string[max=50];
  CLIENT_COUNTRY_NAME:nullable string[max=40];
  ADDRESS_NAME:nullable string[max=250];
  ADDRESS_MISCELLANEOUS:nullable string[max=250];
  ADDRESS_STREET:nullable string[max=250];
  ADDRESS_TOWN:nullable string[max=250];
  ADDRESS_COUNTRY_CODE:nullable string[max=50];
  ADDRESS_COUNTRY_NAME:nullable string[max=40];
  BUSINESS_ACTIVITY_CODE:nullable string[max=50];
  BUSINESS_ACTIVITY_NAME:nullable string[max=250];
  CLIENT_TYPE_CODE:nullable decimal[20,0];
  CLIENT_SEGMENTATION_CODE:nullable decimal[20,0];
  GROUP_NUMBER:nullable decimal[20,0];
  GROUP_NAME:nullable string[max=50];
  CLIENT_MRP:nullable decimal[20,0];
  BDR_ID:nullable string[max=50];
  CLIENT_BCE:nullable decimal[20,0];
  BCE_PORTFOLIO_LEVEL_3:nullable string[max=50];
