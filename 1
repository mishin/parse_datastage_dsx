//
// Generated file to implement the V0S21_Parallel_SG_BCE_ACCRED_T201 transform operator.
//

// define our input/output link names
inputname 0 L201;
outputname 0 L01;
outputname 1 L202;

initialize {
	// define our row rejected variable
	int8 RowRejected0;

	// define our null set variable
	int8 NullSetVar0;

	// declare our intermediate variables for this section
	string InterVar0_0;
	string InterVar0_1;
	string InterVar0_2;
	string InterVar0_3;
	string InterVar0_4;

	// initialise constant values which require conversion
	InterVar0_0 = "400000";
	InterVar0_1 = "600000";
	InterVar0_2 = "R3000";
	InterVar0_3 = "5000000";
	InterVar0_4 = "6000000";
}

mainloop {
	// initialise our row rejected variable
	RowRejected0 = 1;

	// evaluate constraint and columns for link: L01
	if (notnull(L201.LOCAL_CLIENT_ID))
	{
		L01.CTBORINN = trimc_string(L201.LOCAL_CLIENT_ID);
		L01.LOCAL_CLIENT_ID = trimc_string(L201.LOCAL_CLIENT_ID);
		writerecord 0;
		RowRejected0 = 0;
	}
	// evaluate constraint and columns for link: L202
	if (notnull(L201.LOCAL_CLIENT_ID))
	{
		NullSetVar0 = 0;
		if ((((left_substring(trimc_string(L201.LOCAL_CLIENT_ID) , 6) == InterVar0_0) || (left_substring(trimc_string(L201.LOCAL_CLIENT_ID) , 6) == InterVar0_1)) && (right_substring(trimc_string(L201.LOCAL_CLIENT_ID) , 5) == InterVar0_2))) {
			L202.CLIENT_ID = substring_1(trimc_string(L201.LOCAL_CLIENT_ID) , 7 , 5);
		} else {
			if ((((left_substring(trimc_string(L201.LOCAL_CLIENT_ID) , 7) == InterVar0_3) || (left_substring(trimc_string(L201.LOCAL_CLIENT_ID) , 7) == InterVar0_4)) && (string_length(trimc_string(L201.LOCAL_CLIENT_ID)) == 16))) {
				L202.CLIENT_ID = substring_1(trimc_string(L201.LOCAL_CLIENT_ID) , 8 , 6);
			} else {
				NullSetVar0 = 1;
			}
			//;
		}
		L202.CLIENT_ID = (NullSetVar0 == 1) ? set_null() : L202.CLIENT_ID;
		L202.LOCAL_CLIENT_ID = trimc_string(L201.LOCAL_CLIENT_ID);
		writerecord 1;
		RowRejected0 = 0;
	}
}

finish {
}

