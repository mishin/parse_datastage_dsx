//
// Generated file to implement the V0S44_Parallel_SG_BCE_ACCRED_T101 transform operator.
//

// define our input/output link names
inputname 0 L101;
outputname 0 L102;

initialize {
	// define our row rejected variable
	int8 RowRejected0;

	// define our null set variable
	int8 NullSetVar0;

	// declare our intermediate variables for this section
	string InterVar0_0;
	string InterVar0_1;

	// initialise constant values which require conversion
	InterVar0_0 = "RUR";
	InterVar0_1 = "RUB";
}

mainloop {
	// initialise our row rejected variable
	RowRejected0 = 1;

	// evaluate columns (no constraints) for link: L102
	if (notnull(L101.CLIENT_INN)) {
		L102.CTBORINN = trimc_string(L101.CLIENT_INN);
	} else {
		L102.CTBORINN = L101.CLIENT_INN;
	}
	//;
	if ((trimc_string(L101.CCY) == InterVar0_0)) {
		L102.CCY = InterVar0_1;
	} else {
		L102.CCY = L101.CCY;
	}
	//;
	writerecord 0;
	RowRejected0 = 0;
}

finish {
}

