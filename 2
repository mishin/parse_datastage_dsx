  DEVISERE:not_nullable string[max=255]=DEVISERE;
  DATEECH:nullable date=DATEECH;
  MONTANT:nullable decimal[30,2]=MONTANT;
  SIGNEMNT:not_nullable decimal[10,0]=SIGNEMNT;
