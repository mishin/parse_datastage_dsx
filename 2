         Value "50"
      END DSSUBRECORD
      BEGIN DSSUBRECORD
         Name "WHERE_CLAUSE"
      END DSSUBRECORD
      BEGIN DSSUBRECORD
         Name "OTHER_CLAUSES"
      END DSSUBRECORD
      BEGIN DSSUBRECORD
         Name "SQLBUILDERSQL"
      END DSSUBRECORD
      BEGIN DSSUBRECORD
         Name "FULLYGENSQL"
      END DSSUBRECORD
      BEGIN DSSUBRECORD
         Name "USERSQL"
         Value =+=+=+=
SELECT A.CTNUMDOG,  mntExpoCour, decimal(SUMMA,20,2), A.CTENDDATE,B.POCCY,A.CTREPDATE, A.LOCAL_CLIENT_ID,CTCCY,B.POREPDATE FROM 
(SELECT CTREPDATE, CTNUMDOG, mntExpoCour, CTENDDATE, LOCAL_CLIENT_ID,CTCCY FROM #PS_BCE_CONNECTIONS.TRG_SCHEME#.SG_BCE_OPEELEMCREDIT_B_ES WHERE  
mntExpoCour <>0.00 GROUP BY CTREPDATE, CTNUMDOG, mntExpoCour, CTENDDATE, LOCAL_CLIENT_ID,CTCCY) AS A LEFT OUTER JOIN 
(SELECT POCCY,POREPDATE, CTNUMDOG, sum(POAMA) AS SUMMA  FROM (SELECT X.* FROM #PS_BCE_CONNECTIONS.TRG_SCHEME#.SG_BCE_FLP X,#PS_BCE_CONNECTIONS.TRG_SCHEME#.SG_BCE_OPEELEMCREDIT_B_ES Y WHERE X.POTYPE=1 AND X.PODATE>'#DATE#'
and X.CTNUMDOG=Y.CTNUMDOG and X.PODATE<=Y.DATEFIN) 
 GROUP BY POREPDATE, CTNUMDOG, POCCY) AS B ON A.CTNUMDOG=B.CTNUMDOG;
=+=+=+=
      END DSSUBRECORD
      BEGIN DSSUBRECORD
         Name "BEFORESQL"
      END DSSUBRECORD
      BEGIN DSSUBRECORD
         Name "CONTBEFORESQL"
         Value "YES"
      END DSSUBRECORD
      BEGIN DSSUBRECORD
         Name "AFTERSQL"
      END DSSUBRECORD
      BEGIN DSSUBRECORD
         Name "CONTAFTERSQL"
         Value "YES"
      END DSSUBRECORD
      BEGIN DSSUBRECORD
